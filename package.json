{
  "name": "satella",
  "version": "1.0.7",
  "description": "Satella - A Bun backend for Net64 Platform",
  "main": "index.js",
  "scripts": {
    "start": "bun ./index.ts",
    "dev": "bun --watch run ./index.ts",
    "deploy": "bun ./tools/build.ts && node ./tools/deploy.mjs",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "zip": "zip -r satella.zip . -x node_modules/**\\* -x .git/**\\* -x .vscode/**\\* -x .idea/**\\* -x dist/**\\* -x .DS_Store -x .env",
    "upload": "npm run zip && scp satella.zip satella:satella-instance && rm satella.zip"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "helmet": "^8.0.0",
    "id128": "^1.6.6",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.11.15",
    "@types/node-fetch": "^2.6.2",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  },
  "keywords": [],
  "author": "@Net64",
  "license": "ISC"
}
